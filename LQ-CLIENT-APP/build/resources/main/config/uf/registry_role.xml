<?xml version="1.0" encoding="UTF-8"?>
 
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="createRegRoleInfoModel" lazy-init="true" class="com.bjgoodwill.hip.client.app.registry.role.uf.RegRoleInfoModel">
        <property name="fxmlController">
            <bean id="createRegRoleInfoController" lazy-init="true" class="com.bjgoodwill.hip.client.app.registry.role.uf.RegRoleInfoController"/>
        </property>
    </bean>
    <bean id="updateRegRoleInfoModel" lazy-init="true" class="com.bjgoodwill.hip.client.app.registry.role.uf.RegRoleInfoModel">
        <property name="fxmlController">
            <bean id="updateRegRoleInfoController" lazy-init="true" class="com.bjgoodwill.hip.client.app.registry.role.uf.RegRoleInfoController"/>
        </property>
    </bean>
    <bean id="CREATE_REG_ROLE_INFO_DIALOG_MODEL" lazy-init="true" class="com.bjgoodwill.hip.client.fw.root.uf.dialog.DialogModel">
        <property name="title" value="新增角色"></property>
        <property name="subBusinessId" value="createRegRoleInfoModel"></property>
        <property name="width" value="170"></property>
        <property name="heigh" value="270"></property>
        <property name="okBtnText" value="保存"></property>
        <property name="cancelBtnText" value="关闭"></property>
        <property name="okCommand" ref="createRegRoleOkCommand" />
    </bean>
    <bean id="UPDATE_REG_ROLE_INFO_DIALOG_MODEL" lazy-init="true" class="com.bjgoodwill.hip.client.fw.root.uf.dialog.DialogModel">
        <property name="title" value="修改角色"></property>
        <property name="subBusinessId" value="updateRegRoleInfoModel"></property>
        <property name="width" value="170"></property>
        <property name="heigh" value="270"></property>
        <property name="okBtnText" value="保存"></property>
        <property name="cancelBtnText" value="关闭"></property>
        <property name="okCommand" ref="updateRegRoleOkCommand" />
    </bean>
</beans>
