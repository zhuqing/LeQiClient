<BorderPane  prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <HipTableView fx:id="dispensingProvideWinList"  BorderPane.alignment="CENTER">
            <columns>
                <HipTableColumn  prefWidth="140" propertyName="winName" text="发药窗口" >
                    <tableCell>
                        <HipTableTextCell/>
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn prefWidth="140" propertyName="autoprintStatus" text="是否自动打印处方" >
                    <tableCell>
                        <HipCustomDataSourceTableCell >
                            <customDataSourceCallback>
                                <YesAndNOCallback />
                            </customDataSourceCallback>
                        </HipCustomDataSourceTableCell> 
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn prefWidth="140" propertyName="status" text="窗口状态" >
                    <tableCell>
                        <HipCustomDataSourceTableCell >
                            <customDataSourceCallback>
                                <OpenAndCloseCallback />
                            </customDataSourceCallback>
                        </HipCustomDataSourceTableCell> 
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn editable="false" prefWidth="140" text="操作" >
                    <tableCell>
                        <HipTableButtonCell>
                            <buttonList>
                                <ArrayList>
                                    <HipButton alignment="CENTER" text="删除" onAction="#deleteDispensingProvideWin"></HipButton>
                                </ArrayList>
                            </buttonList> 
                        </HipTableButtonCell> 
                    </tableCell>
                </HipTableColumn>
            </columns>
        </HipTableView>
    </center>
    <bottom>
        <VBox prefHeight="300.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <HBox prefHeight="34.0" prefWidth="200.0" styleClass="control-bar-pane">
                    <children>
                        <HipButton  fx:id="addData" mnemonicParsing="false" onAction="#addData" text="保存 " styleClass="save-button" GridPane.columnIndex="0">
                            <graphic>
                                <ImageView pickOnBounds="true" preserveRatio="true"/>
                            </graphic>
                        </HipButton>
                        <HipButton fx:id="clean" mnemonicParsing="false" onAction="#cleanData" text="清空 " styleClass="refresh-button" GridPane.columnIndex="1">
                            <graphic>
                                <ImageView pickOnBounds="true" preserveRatio="true" />
                            </graphic>
                        </HipButton>
                    </children>
                </HBox>
                <HipFormView fx:id="dispensingProvideWinForm" hgap="15.0" prefHeight="131.0" prefWidth="649.0" vgap="12.0">
                    <children>
                        <Label alignment="CENTER_RIGHT" prefHeight="24.0" prefWidth="110.0" text="发药窗口：" GridPane.columnIndex="0" GridPane.rowIndex="1"  />
                            <HipTextInputFormCell prefWidth="140.0" id="winName" propertyName="winName" GridPane.columnIndex="1"   GridPane.rowIndex="1" >
                            </HipTextInputFormCell>

                            <HipBooleanCheckBoxFormCell propertyName="autoprintStatus" prefHeight="24.0" prefWidth="250.0" text="是否自动打印处方" GridPane.rowIndex="3" GridPane.columnIndex="1" GridPane.columnSpan="1"/>

                            <HipBooleanCheckBoxFormCell propertyName="status" prefHeight="24.0" prefWidth="250.0" text="窗口状态" GridPane.rowIndex="4" GridPane.columnIndex="1" GridPane.columnSpan="1"/>
                    </children>
                </HipFormView>
            </children>
        </VBox>
    </bottom>
</BorderPane>

