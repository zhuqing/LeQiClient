<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.util.*?>
<?import java.util.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import com.bjgoodwill.hip.client.fx.control.table.*?>
<?import com.bjgoodwill.hip.client.fx.control.table.cell.*?>
<?import com.bjgoodwill.hip.client.fx.control.button.*?>
<?import com.bjgoodwill.hip.client.fx.callback.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.*?>
<?import com.bjgoodwill.hip.client.app.util.datasource.*?>
<?import com.bjgoodwill.hip.client.app.registry.ets.element.info.uf.*?>
<?import com.bjgoodwill.hip.client.function.HipCallback?>
<?import com.bjgoodwill.hip.client.app.service.ipa.cf.*?>

<SplitPane orientation="VERTICAL" dividerPositions="0.55" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1">
    <items>
        <BorderPane prefHeight="200.0" prefWidth="200.0">
            <top>
                <HBox BorderPane.alignment="CENTER" styleClass="control-bar-pane">
                    <children>
                        <HipButton mnemonicParsing="false" text="新增" onAction="#addNursing" styleClass="add-button">
                            <graphic>
                                <ImageView pickOnBounds="true" preserveRatio="true" />
                            </graphic>
                        </HipButton>
                    </children>
                </HBox>
            </top>
            <center>
                <HipTableView fx:id="serviceNursingView" BorderPane.alignment="CENTER" tableMenuButtonVisible="true">
                    <columns>
                        <HipTableColumn visible="false" editable="false" prefWidth="40" propertyName="id" text="id">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false" prefWidth="265" propertyName="nursingName" text="护理名称">
                            <tableCell>
                                <HipTableTextCell/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false" prefWidth="265" propertyName="areaCode" text="病区">
                            <tableCell>
                                <HipTableTextCell>
                                    <toText>
                                        <IpaIdToOrgNameCallback/>
                                    </toText>
                                </HipTableTextCell>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false" prefWidth="90" propertyName="statusCode" text="状态">
                            <tableCell>
                                <HipTableTextCell/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false" prefWidth="80.0" text="操作">
                            <tableCell>
                                <HipTableButtonCell>
                                    <buttonList>
                                        <ArrayList>
                                            <HipButton text="" onAction="#updateNursing" styleClass="edit-button">
                                                <graphic>
                                                    <ImageView pickOnBounds="true" preserveRatio="true" />
                                                </graphic>
                                            </HipButton>
                                            <HipButton text="" onAction="#deleteNursing" styleClass="delete-button">
                                                <graphic>
                                                    <ImageView pickOnBounds="true" preserveRatio="true" />
                                                </graphic>
                                            </HipButton>
                                        </ArrayList>
                                    </buttonList>
                                </HipTableButtonCell>
                            </tableCell>
                        </HipTableColumn>
                    </columns>
                </HipTableView>
            </center>
        </BorderPane>
        <BorderPane prefHeight="200.0" prefWidth="200.0">
            <top>
                <HBox BorderPane.alignment="CENTER" styleClass="control-bar-pane">
                    <children>
                        <HipButton mnemonicParsing="false" text="新增" onAction="#addNursingItem" styleClass="add-button">
                            <graphic>
                                <ImageView pickOnBounds="true" preserveRatio="true" />
                            </graphic>
                        </HipButton>
                    </children>
                </HBox>
            </top>
            <center>
                <HipTableView fx:id="serviceNursingItemView" BorderPane.alignment="CENTER" tableMenuButtonVisible="true">
                    <columns>
                        <HipTableColumn editable="false" prefWidth="40.0" propertyName="itemCode" text="编  码">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false" prefWidth="200.0" propertyName="itemName" text="名      称">
                            <tableCell>
                                <HipTableTextCell/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="40" propertyName="itemType" text="类  型">
                            <tableCell>
                                <!--<HipTableTextCell/>-->
                                <HipEtsTableCell dictCode="ObsvalueDataType"/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="50.0" propertyName="itemNo" text="顺序">
                            <tableCell>
                                <HipTableTextCell/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="80.0" propertyName="itemUnit" text="单位">
                            <tableCell>
                                <!--<HipTableTextCell/>-->
                                <HipEtsTableCell dictCode="Measures"/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="60.0" propertyName="itemWidth" text="宽度">
                            <tableCell>
                                <HipTableTextCell/>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="200.0" propertyName="itemFormat" text="格      式">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn prefWidth="120.0" propertyName="itemDict" text="字      典">
                            <tableCell>
                                <!--<HipTableTextCell />-->
                                <HipCustomDataSourceTableCell>
                                    <customDataSourceCallback>
                                        <HipCallback beanId="serviceNursingDict"/>
                                    </customDataSourceCallback>
                                </HipCustomDataSourceTableCell>
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="100.0" propertyName="lowLimit" text="危机值下限">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn editable="false"  prefWidth="100.0" propertyName="highLimit" text="危机值下限">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <!--                        <HipTableColumn prefWidth="120.0" propertyName="warnType" text="提醒类型">
                            <tableCell>
                                <HipTableTextCell />
                                <HipCustomDataSourceTableCell>
                                    <customDataSourceCallback>
                                        <HipCallback beanId="serviceNursingDict"/>
                                    </customDataSourceCallback>
                                </HipCustomDataSourceTableCell>
                            </tableCell>
                        </HipTableColumn>-->
                        <HipTableColumn editable="false" prefWidth="80.0" text="操作">
                            <tableCell>
                                <HipTableButtonCell>
                                    <buttonList>
                                        <ArrayList>
                                            <HipButton text="" onAction="#updateNursingItem" styleClass="edit-button">
                                                <graphic>
                                                    <ImageView pickOnBounds="true" preserveRatio="true" />
                                                </graphic>
                                            </HipButton>
                                            <HipButton text="" onAction="#deleteNursingItem" styleClass="delete-button">
                                                <graphic>
                                                    <ImageView pickOnBounds="true" preserveRatio="true" />
                                                </graphic>
                                            </HipButton>
                                        </ArrayList>
                                    </buttonList>
                                </HipTableButtonCell>
                            </tableCell>
                        </HipTableColumn>
                    </columns>
                </HipTableView>
            </center>
        </BorderPane>
    </items>
</SplitPane>
