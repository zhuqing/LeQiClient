<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <left>
        <HipTreeView fx:id="regOrgTreeView" prefWidth="248">
            <hipTreeCell>
                <HipTreeCell propertyName="orgName">
                </HipTreeCell>
            </hipTreeCell>
        </HipTreeView>
    </left>
    <center>
        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <top>
                <BorderPane prefHeight="34.0" prefWidth="824.0" BorderPane.alignment="CENTER">
                    <left>
                        <HipFormView fx:id="filterFormView" alignment="CENTER_LEFT" prefHeight="34.0" prefWidth="440.0" BorderPane.alignment="CENTER">
                            <children>
                                <Label alignment="CENTER_RIGHT" text="对照源:" prefWidth="65" />
                                <HipCustomDataSourceFilterableComboBoxFormCell propertyName="sourceType" prefWidth="146.0" GridPane.columnIndex="1">
                                    <customDataSourceCallback>
                                        <DeptContrastTypeCallBack/>
                                    </customDataSourceCallback>
                                </HipCustomDataSourceFilterableComboBoxFormCell>
                                <Label alignment="CENTER_RIGHT" text="机构名称:" prefWidth="75" GridPane.columnIndex="2"/>
                                <HipTextInputFormCell propertyName="orgName" prefHeight="24.0" prefWidth="120.0" promptText="机构名称过滤" GridPane.columnIndex="3" />
                                <HipButton prefHeight="24.0" prefWidth="80.0" text="查询" tipText="查询" GridPane.columnIndex="4" styleClass="search-button" onAction="#queryContrast">
                                    <graphic>
                                        <ImageView pickOnBounds="true" preserveRatio="true" />
                                    </graphic>
                                </HipButton>
                            </children>
                        </HipFormView>
                    </left>
                    <right>
                        <HBox alignment="CENTER_RIGHT" prefHeight="24.0" prefWidth="440.0" BorderPane.alignment="CENTER">
                            <children>
                                <HipButton mnemonicParsing="false" text="保存" styleClass="save-button" onAction="#saveContrast">
                                    <graphic>
                                        <ImageView pickOnBounds="true" preserveRatio="true" />
                                    </graphic>
                                </HipButton>
                            </children>
                        </HBox>
                    </right>
                </BorderPane>
            </top>
            <center>
                <HipEditTableView fx:id="contrastTableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <hipFormView>
                        <HipFormView>
                            <children>
                                <Label text="" prefWidth="45.0"/>
                                <HipFormTitleCell propertyName="orgCode" GridPane.columnIndex="1"/>
                                <HipFormTitleCell propertyName="orgName" GridPane.columnIndex="2"/>
                                <HipTableViewChooseFormCell propertyName="name" GridPane.columnIndex="3">
                                    <commitPropertyName>
                                        <String fx:value="elementNameXp"/>
                                    </commitPropertyName>
                                    <commitCallback>
                                        <ContrastRegOrgSourceCommitCallback />
                                    </commitCallback>
                                    <queryCallback>
                                        <ContrastRegOrgQueryCallBack />
                                    </queryCallback>
                                    <hipTableView>
                                        <HipTableView maxHeight="196" prefWidth="590" maxWidth="590" styleClass="service-item-dialog">
                                            <columns>
                                                <HipTableColumn propertyName="elementCodeXp" prefWidth="130" text="编码" >
                                                    <tableCell>
                                                        <HipTableTextCell />
                                                    </tableCell>
                                                </HipTableColumn>
                                                <HipTableColumn propertyName="elementNameXp" prefWidth="260" text="名称" >
                                                    <tableCell>
                                                        <HipTableTextCell />
                                                    </tableCell>
                                                </HipTableColumn>
                                            </columns>
                                        </HipTableView>
                                    </hipTableView>
                                </HipTableViewChooseFormCell>
                            </children>
                        </HipFormView>
                    </hipFormView>
                    <columns>
                        <HipTickTableColumn prefWidth="45.0" text="" >
                            <tableCell>
                                <HipCheckBoxTickTableCell/>
                            </tableCell>
                        </HipTickTableColumn>
                        <HipTableColumn prefWidth="100" text="编码" propertyName="orgCode">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn prefWidth="190" text="机构" propertyName="orgName">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn prefWidth="190" text="对照项目" propertyName="name">
                            <tableCell>
                                <HipTableTextCell />
                            </tableCell>
                        </HipTableColumn>
                        <HipTableColumn prefWidth="40.0" text="操作" >
                            <tableCell>
                                <HipTableButtonCell>
                                    <buttonList>
                                        <ArrayList>
                                            <HipButton text="" tipText="删除对照" styleClass="delete-button" onAction="#deleteContrast">
                                                <graphic>
                                                    <ImageView pickOnBounds="true" preserveRatio="true" />
                                                </graphic>
                                            </HipButton>
                                        </ArrayList>
                                    </buttonList>
                                </HipTableButtonCell>
                            </tableCell>
                        </HipTableColumn>
                    </columns>
                </HipEditTableView>
            </center>
        </BorderPane>
    </center>
</BorderPane>
