                        <Label alignment="CENTER_RIGHT" prefWidth="67" text="有效状态:" GridPane.columnIndex="4" GridPane.rowIndex="2" />
                        <HipCustomDataSourceFilterableComboBoxFormCell propertyName="statusCode" prefHeight="24.0" prefWidth="94" GridPane.columnIndex="5" GridPane.rowIndex="2">
                            <customDataSourceCallback>
                                <CustomStatusDataSourceCallback >
                                    <dictElementList>
                                        <ArrayList>
                                            <DictElement elementCode='ACTIVE' elementName='有效' />
                                            <DictElement elementCode='NULLIFIED' elementName='无效' />
                                        </ArrayList>
                                    </dictElementList>
                                </CustomStatusDataSourceCallback>
                            </customDataSourceCallback>
                        </HipCustomDataSourceFilterableComboBoxFormCell>
        
                        <Label alignment="CENTER_RIGHT" prefHeight="24.0" prefWidth="67" text="系      统:" GridPane.rowIndex="3" />
