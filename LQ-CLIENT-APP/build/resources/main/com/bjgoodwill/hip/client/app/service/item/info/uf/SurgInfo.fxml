                        <Label alignment="CENTER_RIGHT" prefHeight="24.0" prefWidth="67" text="项目名称:" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <HipPhoneticizeFormCell propertyName="serviceItemName" phoneticizeName="inputPy" promptText="请输入项目名称" prefHeight="24.0" prefWidth="180" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        
                        <Label alignment="CENTER_RIGHT" prefHeight="24.0" prefWidth="67" text="国家编码:" GridPane.columnIndex="4" GridPane.rowIndex="1" />
