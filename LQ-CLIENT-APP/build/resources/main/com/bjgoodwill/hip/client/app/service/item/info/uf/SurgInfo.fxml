                        <Label alignment="CENTER_RIGHT" prefWidth="67" text="执行科室:" GridPane.columnIndex="4" GridPane.rowIndex="5" />
                        <HipCustomDataSourceFilterableComboBoxFormCell propertyName="defaultExecOrgCode" prefWidth="180" GridPane.columnIndex="5" GridPane.rowIndex="5">
