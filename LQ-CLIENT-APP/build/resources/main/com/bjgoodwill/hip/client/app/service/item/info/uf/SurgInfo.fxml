                        -->
                        <HipCustomDataSourceFilterableComboBoxFormCell fx:id="serviceItemTypeId"  propertyName="serviceItemType" prefHeight="24.0" prefWidth="137" GridPane.columnIndex="1" GridPane.rowIndex="2" >
                            <customDataSourceCallback>
                                <HipCallback beanId="serviceItemTypeCallback"/>
                            </customDataSourceCallback>
                        </HipCustomDataSourceFilterableComboBoxFormCell>
                        <Label alignment="CENTER_RIGHT" prefWidth="67" text="价      格:" GridPane.columnIndex="2" GridPane.rowIndex="2" />
