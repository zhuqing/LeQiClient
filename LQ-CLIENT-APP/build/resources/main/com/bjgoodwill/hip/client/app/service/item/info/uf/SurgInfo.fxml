                                <HipCallback beanId="executeOrgCallback"/>
                            </customDataSourceCallback>
                        </HipCustomDataSourceFilterableComboBoxFormCell>
        
                        <Label alignment="CENTER_RIGHT" prefHeight="24.0" prefWidth="67" text="注意事项:" GridPane.rowIndex="6" />
                        <HipTextInputFormCell propertyName="attention" prefHeight="24.0" prefWidth="566" GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="6" />
        
                        <Label alignment="CENTER_RIGHT" prefHeight="70.0" prefWidth="67" text="项目描述:" GridPane.rowIndex="7" />
                        <HipTextAreaInputFormCell propertyName="remark" minHeight="130.0" prefWidth="627" GridPane.columnIndex="1" GridPane.columnSpan="5" GridPane.rowIndex="7" GridPane.rowSpan="5" rowCount="5"/>

