<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <SplitPane BorderPane.alignment="CENTER">
            <items>
                <SplitPane dividerPositions="0.5" orientation="VERTICAL">
                    <items>
                        <HipTableView fx:id="patRcmcParmTableView" prefHeight="330.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
                            <columns>
                                <HipTableColumn prefWidth="130" propertyName="feeType" text="农合名称">
                                    <tableCell>
                                        <HipEtsTableCell dictCode="ChargeType"/>
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="patRcmcDiaLog" text="农合弹出框">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="100" propertyName="userName" text="用户名">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="100" propertyName="password" text="密码">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="100" propertyName="centerNo" text="农合中心编码">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="100" propertyName="hospCode" text="农合分配编码">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="statusCode" text="是否有效">
                                    <tableCell>
                                        <HipCustomDataSourceTableCell >
                                            <customDataSourceCallback>
                                                <CustomStatusDataSourceCallback >
                                                    <dictElementList>
                                                        <ArrayList>
                                                            <DictElement elementCode='ACTIVE' elementName='有效' />
                                                            <DictElement elementCode='NULLIFIED' elementName='无效' />
                                                        </ArrayList>
                                                    </dictElementList>
                                                </CustomStatusDataSourceCallback>
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceTableCell>
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn editable="false" prefWidth="80" text="操作">
                                    <tableCell>
                                        <HipTableButtonCell>
                                            <buttonList>
                                                <ArrayList>
                                                    <HipButton onAction="#PingUrl" text="测试网络" styleClass="edit-button">
                                                        <graphic>
                                                            <ImageView pickOnBounds="true" preserveRatio="true" />
                                                        </graphic>
                                                    </HipButton>
                                                </ArrayList>
                                            </buttonList>
                                        </HipTableButtonCell>
                                    </tableCell>
                                </HipTableColumn>
                            </columns>
                        </HipTableView>
                        <BorderPane>
                            <top>
                                <HBox alignment="CENTER_LEFT"  prefHeight="34.0" prefWidth="1024.0" styleClass="control-bar-pane">
                                    <children>
                                        <TextField fx:id="filterField" promptText="备注过滤" prefWidth="120" prefHeight="26"/>
                                        <HipButton onAction="#CreatePatRcmcParm" text="新增" styleClass="add-button" >
                                            <graphic>
                                                <ImageView pickOnBounds="true" preserveRatio="true" />
                                            </graphic>
                                        </HipButton>
                                        <HipButton onAction="#SavePatRcmcParm" text="保存" styleClass="save-button" >
                                            <graphic>
                                                <ImageView pickOnBounds="true" preserveRatio="true" />
                                            </graphic>
                                        </HipButton>
                                        <HipButton onAction="#QueryPatRcmcParm" text="查询全表" styleClass="search-button" >
                                            <graphic>
                                                <ImageView pickOnBounds="true" preserveRatio="true" />
                                            </graphic>
                                        </HipButton>
                                    </children>
                                </HBox>
                            </top>
                            <center>
                                <HipFormView fx:id="patRcmcParmFormView" prefHeight="329.0" prefWidth="732.0">
                                    <children>
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="费别:" GridPane.columnIndex="0" />
                                        <HipEtsFilterableComboBoxFormCell prefHeight="30.0" prefWidth="180" propertyName="feeType"  GridPane.columnIndex="1" dictCode="ChargeType" />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="农合弹出框：" GridPane.columnIndex="2"/>
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="patRcmcDiaLog"  GridPane.columnIndex="3" />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="农合医院编码：" GridPane.columnIndex="4" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="hospCode" GridPane.columnIndex="5" />
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="用户名："  GridPane.rowIndex="1" GridPane.columnIndex="0" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="userName"   GridPane.rowIndex="1" GridPane.columnIndex="1"/>
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="密码：" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="password"  GridPane.rowIndex="1" GridPane.columnIndex="3" />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="农合中心号：" GridPane.rowIndex="1" GridPane.columnIndex="4"/>
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="centerNo" GridPane.rowIndex="1" GridPane.columnIndex="5" />
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="115.0" text="最大下载流水号："  GridPane.rowIndex="2" GridPane.columnIndex="0" />
                                        <HipTextInputFormCell  prefHeight="30.0" prefWidth="180" propertyName="ssMaxXzlsh" GridPane.rowIndex="2" GridPane.columnIndex="1" />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="备注：" GridPane.rowIndex="2" GridPane.columnIndex="2" />
                                        <HipTextInputFormCell  prefHeight="30.0" prefWidth="180" propertyName="remark" GridPane.rowIndex="2" GridPane.columnIndex="3" />
                                        <Label alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="90.0" text="有效标记："  GridPane.rowIndex="2" GridPane.columnIndex="4" />
                                        <HipCustomDataSourceFilterableComboBoxFormCell prefHeight="35.0" prefWidth="180" propertyName="statusCode" GridPane.rowIndex="2" GridPane.columnIndex="5">
                                            <customDataSourceCallback>
                                                <CustomStatusDataSourceCallback>
                                                    <dictElementList>
                                                        <ArrayList>
                                                            <DictElement elementCode="ACTIVE" elementName="有效" />
                                                            <DictElement elementCode="NULLIFIED" elementName="无效" />
                                                        </ArrayList>
                                                    </dictElementList>
                                                </CustomStatusDataSourceCallback>
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceFilterableComboBoxFormCell>
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="是否是门诊：" GridPane.rowIndex="3" GridPane.columnIndex="0" />
                                        <HipCustomDataSourceFilterableComboBoxFormCell prefHeight="30.0" prefWidth="90.0" propertyName="type" GridPane.rowIndex="3" GridPane.columnIndex="1" >
                                            <customDataSourceCallback>
                                                <!--<ManufactureFirmCallback/>-->
                                                <AmbOrImpCallback/>
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceFilterableComboBoxFormCell>
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="农合名称：" GridPane.rowIndex="3" GridPane.columnIndex="2" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="200" propertyName="hisId" GridPane.rowIndex="3" GridPane.columnIndex="3"/>
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="IP地址：" GridPane.rowIndex="3" GridPane.columnIndex="4" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="200" propertyName="ip" GridPane.rowIndex="3" GridPane.columnIndex="5"/>
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="农合地址：" GridPane.rowIndex="5" GridPane.columnIndex="0" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="380" propertyName="url" GridPane.rowIndex="5" GridPane.columnIndex="1"  GridPane.columnSpan="3"/>
                                   
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="农合后台地址：" GridPane.rowIndex="6" GridPane.columnIndex="0" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="380" propertyName="backGroundUrl" GridPane.rowIndex="6" GridPane.columnIndex="1"  GridPane.columnSpan="3"/>
                                    </children>
                                </HipFormView>
                            </center>
                        </BorderPane>
                    </items>
                </SplitPane>
            </items>
        </SplitPane>
    </center>
</BorderPane>
