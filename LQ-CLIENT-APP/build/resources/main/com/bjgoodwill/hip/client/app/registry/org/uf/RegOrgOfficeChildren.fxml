<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <HipTableView fx:id="regOrgTableView">
            <columns>
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="orgCode" text="科室编码">
                    <tableCell>
                        <HipTableTextCell />
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="orgName" text="科室名称">
                    <tableCell>
                        <HipTableTextCell />
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="orgServiceTypeCode" text="科室类型">
                    <tableCell>
                        <HipEtsTableCell dictCode="SdlocType" />
                    </tableCell>
                </HipTableColumn>
                
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="orgTypeCode" text="科室分类">
                    <tableCell>
                        <HipEtsTableCell dictCode="DeptType" />
                    </tableCell>
                </HipTableColumn>
                
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="adminTypeCode" text="临床类型">
                    <tableCell>
                        <HipEtsTableCell dictCode="DeptISAttr"/>
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="orgAddr" text="科室地址">
                    <tableCell>
                        <HipTableTextCell />
                    </tableCell>
                </HipTableColumn>
                <HipTableColumn editable="false" prefWidth="140.0" propertyName="statusCode" text="有效标记">
                    <tableCell>
                        <HipCustomDataSourceTableCell >
                            <customDataSourceCallback>
                                <CustomStatusDataSourceCallback >
                                    <dictElementList>
                                        <ArrayList>
                                            <DictElement elementCode='ACTIVE' elementName='有效' />
                                            <DictElement elementCode='NULLIFIED' elementName='无效' />
                                        </ArrayList>
                                    </dictElementList>
                                </CustomStatusDataSourceCallback>
                            </customDataSourceCallback>
                        </HipCustomDataSourceTableCell>
                    </tableCell>
                </HipTableColumn>
            </columns>
        </HipTableView>
        </top>
        <center>
        <BorderPane>
            <top>
                <HBox styleClass="control-bar-pane">
                    <children>
                        <HipButton onAction="#createRegOrgCommand" prefHeight="36.0" prefWidth="74.0" text="新增" styleClass="add-button" >
                            <graphic>
                                <ImageView pickOnBounds="true" preserveRatio="true" />
                            </graphic>
                        </HipButton>
                        <HipButton onAction="#saveRegOrgCommand" prefHeight="36.0" prefWidth="74.0" text="保存" styleClass="save-button" >
                            <graphic>
                                <ImageView pickOnBounds="true" preserveRatio="true" />
                            </graphic>
                        </HipButton>
                    </children>
                </HBox>
            </top>
            <center>
                <HipFormView fx:id="regOrgFormView"  prefHeight="329.0" prefWidth="732.0">
                    <children>
                        <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="科室编码" />
                        <HipFormCell prefHeight="30.0" prefWidth="200.0" propertyName="orgCode" GridPane.columnIndex="1" />
                        <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="科室名称" GridPane.columnIndex="2" />
                        <HipPhoneticizeFormCell promptText="请输入机构名称" prefHeight="30.0" prefWidth="200.0" propertyName="orgName" phoneticizeName="inputPy" GridPane.columnIndex="3" />
                        <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="科室类型" GridPane.columnIndex="2" GridPane.rowIndex="1" />
<!--                        <HipEtsFilterableComboBoxFormCell prefHeight="30.0" prefWidth="200.0" propertyName="orgServiceTypeCode" GridPane.columnIndex="3" GridPane.rowIndex="1" dictCode="SdlocType" />-->
                        <HipCustomDataSourceFilterableComboBoxFormCell fx:id="orgServiceTypeCodeId"  propertyName="orgServiceTypeCode" prefHeight="24.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="1" >
                            <customDataSourceCallback>
                                <HipCallback beanId="sdlocTypeForOfficeCallback"/>
                            </customDataSourceCallback>
                        </HipCustomDataSourceFilterableComboBoxFormCell>
                        
                         <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="科室分类" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                        <HipEtsFilterableComboBoxFormCell prefHeight="30.0" prefWidth="200.0" propertyName="orgTypeCode" GridPane.columnIndex="1" GridPane.rowIndex="1" dictCode="DeptType" />
                        
                        
                        <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="临床类型" GridPane.rowIndex="2" />
                        <HipEtsFilterableComboBoxFormCell prefHeight="30.0" prefWidth="200.0" propertyName="adminTypeCode" GridPane.columnIndex="1" GridPane.rowIndex="2" dictCode="DeptISAttr"/>
                        <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="科室地址" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                        <HipTextInputFormCell prefHeight="30.0" prefWidth="200.0" promptText="请输入科室地址" propertyName="orgAddr" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                        <HipFormTitleCell alignment="CENTER_RIGHT" prefWidth="70.0" prefHeight="30.0" text="科室状态" GridPane.rowIndex="3" />
                        <HipCustomDataSourceFilterableComboBoxFormCell prefHeight="30.0" prefWidth="200.0" propertyName="statusCode" GridPane.columnIndex="1" GridPane.rowIndex="3">
                            <customDataSourceCallback>
                                <CustomStatusDataSourceCallback >
                                    <dictElementList>
                                        <ArrayList>
                                            <DictElement elementCode='ACTIVE' elementName='有效' />
                                            <DictElement elementCode='NULLIFIED' elementName='无效' />
                                        </ArrayList>
                                    </dictElementList>
                                </CustomStatusDataSourceCallback>
                            </customDataSourceCallback>
                        </HipCustomDataSourceFilterableComboBoxFormCell>
                    </children>
                </HipFormView>
            </center>
        </BorderPane>
        </center>
</BorderPane>