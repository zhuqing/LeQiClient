<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <SplitPane BorderPane.alignment="CENTER">
            <items>
                <SplitPane dividerPositions="0.5" orientation="VERTICAL">
                    <items>
                        <HipTableView fx:id="hipStorageServiceTableView" prefHeight="330.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
                            <columns>
                                <HipTableColumn prefWidth="130" propertyName="storageName" text="库存管理单位">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="orgName" text="所属机构">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="localeName" text="地点">
                                    <tableCell>
                                        <HipTableTextCell />
                                    </tableCell>
                                </HipTableColumn>
                               
                               
                                <HipTableColumn prefWidth="130" propertyName="storageType" text="库存类型">
                                    <tableCell>
                                        <HipEtsTableCell dictCode="StockType" />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="serviceType" text="服务类型">
                                    <tableCell>
                                        <HipEtsTableCell dictCode="OrgServiceType" />
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="statusCode" text="是否有效">
                                    <tableCell>
                                        <HipCustomDataSourceTableCell >
                                            <customDataSourceCallback>
                                                <CustomStatusDataSourceCallback >
                                                    <dictElementList>
                                                        <ArrayList>
                                                            <DictElement elementCode='ACTIVE' elementName='有效' />
                                                            <DictElement elementCode='NULLIFIED' elementName='无效' />
                                                        </ArrayList>
                                                    </dictElementList>
                                                </CustomStatusDataSourceCallback>
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceTableCell>
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="isManager" text="是否管理库存">
                                    <tableCell>
                                        <HipTableTextCell>
                                            <toText>
                                                <IsYesOrNoCallback />
                                            </toText>
                                        </HipTableTextCell>
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="isBatch" text="是否批次管理">
                                    <tableCell>
                                        <HipTableTextCell>
                                            <toText>
                                                <IsYesOrNoCallback />
                                            </toText>
                                        </HipTableTextCell>
                                    </tableCell>
                                </HipTableColumn>
                                <HipTableColumn prefWidth="130" propertyName="effectiveDate" text="有效期">
                                    <tableCell>
                                        <HipDataTimeTableCell dateFormat="yyyy-MM-dd">
                                        </HipDataTimeTableCell> 
                                    </tableCell>
                                </HipTableColumn>
                
                                <HipTableColumn editable="false" prefWidth="150.0" text="操作">
                                    <tableCell>
                                        <HipTableButtonCell>
                                            <buttonList>
                                                <ArrayList>
                                                    <HipButton onAction="#deleteStorage" text="删除">
                                                    </HipButton>
                                                </ArrayList>
                                            </buttonList>
                                        </HipTableButtonCell> 
                                    </tableCell>
                                </HipTableColumn>
                            </columns>
                        </HipTableView>
                        <BorderPane>
                            <top>
                                <HBox alignment="CENTER_LEFT"  prefHeight="34.0" prefWidth="1024.0" styleClass="control-bar-pane">
                                    <children>
                                        <HipButton onAction="#AddStorage" text="新增" styleClass="add-button" >
                                            <graphic>
                                                <ImageView pickOnBounds="true" preserveRatio="true" />
                                            </graphic>
                                        </HipButton>
                                        <HipButton onAction="#SaveStorage" text="保存" styleClass="save-button" >
                                            <graphic>
                                                <ImageView pickOnBounds="true" preserveRatio="true" />
                                            </graphic>
                                        </HipButton>
                                    </children>
                                </HBox>
                            </top>
                            <center>
                                <HipFormView fx:id="ServiceStorageFormView" prefHeight="329.0" prefWidth="732.0">
                                    <children>
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="名称:" />
                                        <HipCascadeFieldFormCell defTabName="所属库房" prefWidth="180" propertyName="storageCode" textPropertyName="orgName" valuePropertyName="id"  GridPane.columnIndex="1"  >
                                            <customDataSourceCallback>
                                                <StorageOrgTreeSourceCallback />
                                            </customDataSourceCallback>
                                        </HipCascadeFieldFormCell>
                                        
                                        <HipCustomDataSourceFilterableComboBoxFormCell fx:id="comBoxName" prefHeight="30" prefWidth="180" propertyName="storageCode" GridPane.columnIndex="1">
                                            <customDataSourceCallback>
                                                <StorageSourceCallback />
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceFilterableComboBoxFormCell>
                                        <!--                                        <Label alignment="CENTER" prefHeight="15.0" prefWidth="90.0" text="地点:" GridPane.columnIndex="2" />
                                                                                <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="localeName" GridPane.columnIndex="3" />
                                        <HipCustomDataSourceFilterableComboBoxFormCell prefHeight="30" prefWidth="180" propertyName="localeId" GridPane.columnIndex="3">
                                            <customDataSourceCallback>
                                                <StorageLocaleSourceCallback/>
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceFilterableComboBoxFormCell>-->
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="机构:" GridPane.columnIndex="2" />
                                        <!--                                        <HipCustomDataSourceFilterableComboBoxFormCell prefWidth="180" propertyName="orgName"   GridPane.columnIndex="5" >
                                            <customDataSourceCallback>
                                                <StorageOrgTreeSourceCallback />
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceFilterableComboBoxFormCell>-->
                                        <HipCascadeFieldFormCell defTabName="所属组织" prefWidth="180" id = "orgName" textPropertyName="orgName" valuePropertyName="orgCode"   propertyName="orgCode" GridPane.columnIndex="3">
                                            <customDataSourceCallback>
                                                <StorageOrgTreeSourceCallback />
                                            </customDataSourceCallback>
                                        </HipCascadeFieldFormCell>
                                       
                                        <Label alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="90.0" text="库存类型:" GridPane.rowIndex="0" GridPane.columnIndex="4" />
                                        <HipEtsFilterableComboBoxFormCell dictCode="StockType" prefHeight="30.0" prefWidth="180" propertyName="storageType" GridPane.rowIndex="0" GridPane.columnIndex="5" />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="服务类型:" GridPane.rowIndex="1"/>
                                        <HipEtsFilterableComboBoxFormCell dictCode="OrgServiceType" prefHeight="30.0" prefWidth="180" propertyName="serviceType" GridPane.rowIndex="1" GridPane.columnIndex="1" />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="药品类型:" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
                                        <HipEtsFilterableComboBoxFormCell dictCode="DrugType" prefHeight="30.0" prefWidth="180" propertyName="drugType" GridPane.rowIndex="1" GridPane.columnIndex="3"/>
                                        <!--<Label alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="90.0" text="是否批次管理:" GridPane.rowIndex="3" />--> 
                                        <HipBooleanRadioFormCell text="是否批次管理" prefHeight="30.0" prefWidth="180" propertyName="isBatch" GridPane.rowIndex="1" GridPane.columnIndex="5"/>
                                        <!--                                        <Label alignment="CENTER_RIGHT" text="是否管理库存:" prefWidth="90.0" GridPane.columnIndex="2" GridPane.rowIndex="3" />-->
                                        <HipBooleanRadioFormCell text="是否管理库存" prefHeight="25.0" prefWidth="180" propertyName="isManager" GridPane.rowIndex="2" GridPane.columnIndex="1" />
                                        <Label alignment="CENTER_RIGHT" text="是否有效:" prefWidth="90.0"  GridPane.rowIndex="2" GridPane.columnIndex="2" />
                                        <HipCustomDataSourceFilterableComboBoxFormCell prefHeight="30" prefWidth="180" propertyName="statusCode" GridPane.rowIndex="2" GridPane.columnIndex="3" >
                                            <customDataSourceCallback>
                                                <CustomStatusDataSourceCallback>
                                                    <dictElementList>
                                                        <ArrayList>
                                                            <DictElement elementCode="ACTIVE" elementName="有效" />
                                                            <DictElement elementCode="NULLIFIED" elementName="无效" />
                                                        </ArrayList>
                                                    </dictElementList>
                                                </CustomStatusDataSourceCallback>
                                            </customDataSourceCallback>
                                        </HipCustomDataSourceFilterableComboBoxFormCell>
                                        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="90.0" text="有效时间:" GridPane.rowIndex="2" GridPane.columnIndex="4" />
                                        <HipCalendarFormCell dateFormat="yyyy-MM-dd" prefHeight="30.0" prefWidth="180" propertyName="effectiveDate" GridPane.rowIndex="2" GridPane.columnIndex="5" />
                                        <!--                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="结束时间:" GridPane.rowIndex="3"/>
                                                   <HipCalendarFormCell dateFormat="yyyy-MM-dd" prefHeight="30.0" prefWidth="180" propertyName="effectiveHighDate" GridPane.rowIndex="3" GridPane.columnIndex="1" />
                                        -->
                                        
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="拼音码:"  GridPane.rowIndex="3" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="inputPy" GridPane.rowIndex="3" GridPane.columnIndex="1"  />
                                        <Label alignment="CENTER_RIGHT" prefWidth="90.0" text="助记码:" GridPane.rowIndex="3" GridPane.columnIndex="2" />
                                        <HipTextInputFormCell prefHeight="30.0" prefWidth="180" propertyName="mnemonicCode" GridPane.rowIndex="3" GridPane.columnIndex="3"/>
                                        
                                    </children>
                                </HipFormView>
                            </center>
                        </BorderPane>
                    </items>
                </SplitPane>
            </items>
        </SplitPane>
    </center>
</BorderPane>
