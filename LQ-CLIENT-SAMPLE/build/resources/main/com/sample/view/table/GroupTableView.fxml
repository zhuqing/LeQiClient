<HipGroupTableView xmlns:fx="http://javafx.com/fxml/1" groupHipTableCellId="name"  >
    
    <invisableTableCellIds>
        <String fx:value="id"></String>
        <String fx:value="gender"></String>
        <String fx:value="tick"></String>
    </invisableTableCellIds>
    <rowTipTextCallback>
        <RowTipTextCallback/>
    </rowTipTextCallback>
    <columns>
        
        <HipTickTableColumn   >
            <tableCell>
                <HipTreeItemCheckBoxTickTableCell >
                    
                </HipTreeItemCheckBoxTickTableCell>
            </tableCell>
           
        </HipTickTableColumn>
        
        <HipTableColumn propertyName="value.dictCode" text="Name" >
            <tableCell>
                <HipTableTextCell fx:id="name" >
                    
                </HipTableTextCell>
            </tableCell>
        </HipTableColumn>
        
        <HipTableColumn propertyName="value.id"   text="Id">
            <tableCell>
                <HipTableTextCell fx:id="id" >
                    
                </HipTableTextCell>
            </tableCell>
        </HipTableColumn>
        
        <HipTableColumn propertyName="value.inputPy"   text="gender">
            <tableCell>
                <HipCheckBoxTableCell fx:id="gender" >
                    
                </HipCheckBoxTableCell>
            </tableCell>
        </HipTableColumn>
    </columns>
</HipGroupTableView>
